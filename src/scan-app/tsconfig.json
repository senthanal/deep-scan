{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../../dist/scan-app",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "es6",
    "module": "ES6",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "files": [],
  "include": ["./**/*.ts", "./**/*.tsx"],
  "exclude": ["**/*.spec.ts", "**/*.test.ts"]
}
